
<div class="center jumbotron"> <!-- is a BOOTSTARAP thing -->
  <%provide(:title, "Session Sign_In or New User Sign_Up")%>
  <h1> Welcome to the Sample App </h1>
  <h1> This is the Homepage used for the Ruby on Rails Tutorial </h1>
  <h1> the html is called home.html this is not MY HOME </h1>
  <h1> this is found in views/sessions/new.html.erb </h1>
  <h1> localhost:3000/sign_in </h1>

<div class="col-md-6 col-md-offset-3">

<h1>Session Sign-In</h1>

<%= form_tag do %>

  <div class="form-group">
    <%= label_tag :email %>
    <%= text_field_tag :email, "", class: "form-control" %>
  </div>

  <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password, "", class: "form-control" %>
  </div>

  <div class="form-group">
    <%= submit_tag "Sign In" %> or <%=link_to "Sign Up", sign_up_path %>
  </div>

  <% end %>

  <div class="tinybox">
    <%= image_tag("hero.jpg", alt: "what_happens") %>
  </div> <!-- closes the tiny box -->

</div>      <!-- closes the col - md - 6 div -->

</div> <!-- closes the jumbotron -->
