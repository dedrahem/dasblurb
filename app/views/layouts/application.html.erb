<!DOCTYPE html>
<html>
<head>
  <title>Dasblurb</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div class="container">
    <% if @current_user %>
      <div>
        <%= link_to 'Sign-Out', sign_out_path, method: :delete %>
      </div>
      <% end %>
  <%= yield %>
  </div>

</body>

</html>
